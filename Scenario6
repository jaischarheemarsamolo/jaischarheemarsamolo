package scenario6;
import java.util.Scanner;

public class Scenario6 {

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);

       System.out.println("First number:");
       int a = sc.nextInt();
       System.out.println("Second number:");
       int b = sc.nextInt();

       int c ;
       c = a-b;

       if(a==b) {
                 System.out.println("First number and Second number are the same");
       }
       else {
            System.out.println("First number and Second number are not the same");

       }   
    }
